    <script>
    function dangxuat() {
        var r = confirm("Bạn có muốn đăng xuất không?");
        if (r)
        {
            $.ajax({
                type: "GET",
                url: "/Account/Logout",
                dataType: "json",
                success: function (data) {
                    Tham_so = null;
                    User_save = null;
                    window.location.href = "@Url.Action("Index", "Account")";
                },
                error: function (err) {
                    alert('Chúng tôi chưa thể kết nối đến máy chủ lúc này :(');
                }
            });
        }
    };
    function close_loading_screen() {
        $('#loadingmodal').modal('hide');
    }
    function open_loading_screen(title) {
        if (title == "") {
            $('#LoadingTitle').html("Đang kết nối...");
        } else {
            $('#LoadingTitle').html(title);
        }
        $('#loadingmodal').modal('show');
    };
    $(document).ready(function () {
        open_loading_screen();
        $.ajax({
            type: "GET",
            url: "/Admin/GetUser",
            dataType: "json",
            success: function (data) {
                close_loading_screen();
                User_save = data;
                $('#User_title').html(User_save.HoTen);
            }
        });
    });
    function GetDate(str) {
        if (str == null)
            return "";
        var e = str.lastIndexOf(")/");
        var s = str.lastIndexOf("/Date(") + 6;
        var date = new Date(Number(str.substring(s, e)));
        var string = String(date.getDate()).padStart(2, '0')  + '-' + String(date.getMonth() + 1).padStart(2, '0') + '-' + date.getFullYear();
        if (date.getFullYear() < 1000) {
            return "";
        }
        else {
            return string;
        }
    }
    function doimatkhau() {
        var mk_cu = $('#mat_khau_cu').val();
        var mk_moi = $('#mat_khau_moi').val();
        var mk_moi2 = $('#mat_khau_moi2').val();
        if (mk_cu == "" || mk_moi == "" || mk_moi2 == "") {
            alert("Bạn cần nhập đầy đủ thông tin để đổi mật khẩu!");
        }
        else {
            if (mk_moi != mk_moi2) {
                alert("Mật khẩu không trùng khớp, vui lòng nhập lại!");
            }
            else {
                $.ajax({
                    type: "POST",
                    url: "/Admin/ChangePassWordUser",
                    data: {
                        'PasswordOld': mk_cu,
                        'PasswordNew': mk_moi
                    },
                    success: function (data) {
                            alert(data.Message);

                    }
                });
            }
        }

    }
    </script>
    <script>
        function sortByKeyDesc(array, key) {
            return array.sort(function (a, b) {
                var x = parseInt(a[key]); var y = parseInt(b[key]);
                return ((x > y) ? -1 : ((x < y) ? 1 : 0));
            });
        }
        function sortByKeyAsc(array, key) {
            return array.sort(function (a, b) {
                var x = parseInt(a[key]); var y = parseInt(b[key]);
                return ((x < y) ? -1 : ((x > y) ? 1 : 0));
            });
        }
    </script>
